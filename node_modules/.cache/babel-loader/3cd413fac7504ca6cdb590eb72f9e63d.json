A;AAAA;;AAAA;AAAA;AAAA;AAQA,cAAA,OAAA,CAAA,KAAA;;AARA;AAWA,cAAA,MAAA,CAAA,MAAA,GAAA,KAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,GAzEA;AA2EA,EAAA,OAAA,EAAA;AAEA,IAAA,WAFA,uBAEA,MAFA,EAEA;AACA,WAAA,OAAA,GAAA,MAAA;AACA,WAAA,SAAA,GAAA,KAAA,WAAA,CAAA,SAAA;AACA,WAAA,QAAA,GAAA,KAAA,WAAA,CAAA,QAAA;AACA,WAAA,KAAA,GAAA,KAAA,WAAA,CAAA,KAAA;AACA,WAAA,MAAA,GAAA,KAAA,WAAA,CAAA,MAAA;AACA,WAAA,KAAA,GAAA,KAAA,WAAA,CAAA,KAAA;AACA,WAAA,KAAA,GAAA,KAAA,WAAA,CAAA,KAAA;AACA,WAAA,IAAA,GAAA,KAAA,WAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,EAAA,OAAA,CAAA,GAAA,EAAA,GAAA,CAAA;AACA,WAAA,GAAA,GAAA,KAAA,WAAA,CAAA,GAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAbA;AAeA,IAAA,WAfA,yBAeA;AACA,WAAA,WAAA,CAAA,SAAA,GAAA,KAAA,SAAA;AACA,WAAA,WAAA,CAAA,QAAA,GAAA,KAAA,QAAA;AACA,WAAA,WAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,WAAA,WAAA,CAAA,MAAA,GAAA,KAAA,MAAA;AACA,WAAA,WAAA,CAAA,KAAA,GAAA,KAAA,KAAA,CAAA,QAAA,EAAA;AACA,WAAA,WAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,WAAA,WAAA,CAAA,IAAA,GAAA,KAAA,IAAA;AACA,WAAA,WAAA,CAAA,GAAA,GAAA,KAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AACA,MAAA,iBAAA,CAAA,MAAA,CAAA,KAAA,WAAA,CAAA,EAAA,EAAA,KAAA,WAAA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,KA3BA;AA4BA,IAAA,IA5BA,gBA4BA,IA5BA,EA4BA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA;AA9BA;AA3EA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-content>\n      <v-container fluid>\n        <v-row align=\"center\" justify=\"center\">\n          <v-col cols=\"auto\">\n            <v-card elevation=\"10\" max-height=\"400\" max-width=\"400\">\n              <v-img class=\"user \" :src=\"profileData.img\" max-height=\"400\" max-width=\"400\"></v-img>\n            </v-card>\n            <v-card class=\"mt-2\" elevation=\"10\">\n              <v-card-title class=\"justify-center\" style=\"font-size:2em\">{{ profileData.firstName }}\n                {{ profileData.lastName }}\n              </v-card-title>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" sm=\"6\" md=\"8\">\n            <v-card elevation=\"10\">\n              <v-card-title class=\"ml-5\" style=\"font-size:2em\">User Profile</v-card-title>\n              <v-row class=\"mb-2\">\n                <v-col cols=\"12\" md=\"6\">\n                  <v-card-title class=\"ml-10 mr-10 mb-2\">First Name</v-card-title>\n                  <v-card-subtitle class=\"dateProfile\" style=\"font-size:18px\">{{profileData.firstName }}</v-card-subtitle>\n                  <v-card-title class=\"ml-10 mr-10 mb-2\">Last Name</v-card-title>\n                  <v-card-subtitle class=\"dateProfile\" style=\"font-size:18px\">{{profileData.lastName }}</v-card-subtitle>\n                  <v-row>\n                    <v-col>\n                      <v-card-title class=\"ml-10 mr-10 mb-2\">Gender</v-card-title>\n                      <v-card-subtitle class=\"dateProfile\" style=\"font-size:18px\">{{ profileData.gender }}</v-card-subtitle>\n                    </v-col>\n                    <v-divider insert vertical></v-divider>\n                    <v-col>\n                      <v-card-title class=\"ml-10 mr-10 mb-2\">State</v-card-title>\n                      <v-card-subtitle class=\"dateProfile\" style=\"font-size:18px\">{{ profileData.state }}</v-card-subtitle>\n                    </v-col>\n                  </v-row>\n                </v-col>\n                <v-divider inset vertical></v-divider>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-card-title class=\"ml-10 mr-10 mb-2\">E-mail</v-card-title>\n                  <v-card-subtitle class=\"dateProfile\" style=\"font-size:1em\">{{ profileData.email }}</v-card-subtitle>\n                  <v-card-title class=\"ml-10 mr-10 mb-2\">Birthday Date</v-card-title>\n                  <v-card-subtitle class=\"dateProfile\" style=\"font-size:1em\">{{ profileData.date.slice(0, 10).replace('T', ' ')}}</v-card-subtitle>\n                  <v-card-title class=\"ml-10 mr-10 mb-2\">Phone</v-card-title>\n                  <v-card-subtitle class=\"dateProfile\" style=\"font-size:18px\">{{ profileData.phone }}</v-card-subtitle>\n                </v-col>\n              </v-row>\n              <v-divider></v-divider>\n              <v-card-actions>\n                <v-row>\n                  <v-btn class=\"ma-6\" color=\"#BBDEFB\" elevation=\"2\" @click=\"editProfile(profileData)\">Edit profile</v-btn>\n                </v-row>\n              </v-card-actions>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-content>\n    <template>\n      <v-dialog v-model=\"dialog\" persistent max-width=\"500px\" v-if=\"correct!=null\">\n        <v-card>\n          <v-card-title>\n            <span class=\"text-h5\">User Profile</span>\n          </v-card-title>\n          <v-card-text>\n\n            <v-row align=\"center\" justify=\"center\">\n              <v-col cols=\"12\" sm=\"8\">\n                <v-form>\n                  <v-text-field\n                      outlined dense color=\"blue\"\n                      class=\"mt-16\"\n                      v-model=\"firstName\"\n                      :error-messages=\"firstNameErrors\"\n                      :counter=\"30\"\n                      label=\"First Name\"\n                      prepend-icon=\"mdi-account-details\"\n                      required\n                      @input=\"$v.firstName.$touch()\"\n                      @blur=\"$v.firstName.$touch()\"\n                  ></v-text-field>\n                  <v-text-field\n                      outlined dense color=\"blue\"\n                      v-model=\"lastName\"\n                      :error-messages=\"lastNameErrors\"\n                      :counter=\"30\"\n                      label=\"Last Name\"\n                      prepend-icon=\"mdi-account-details-outline\"\n                      required\n                      @input=\"$v.lastName.$touch()\"\n                      @blur=\"$v.lastName.$touch()\"\n                  ></v-text-field>\n                  <v-text-field\n                      outlined dense color=\"blue\"\n                      v-model=\"email\"\n                      :error-messages=\"emailErrors\"\n                      label=\"E-mail\"\n                      prepend-icon=\"mdi-at\"\n                      required\n                      @input=\"$v.email.$touch()\"\n                      @blur=\"$v.email.$touch()\"\n                  ></v-text-field>\n                  <v-text-field\n                      outlined dense color=\"blue\"\n                      v-model=\"img\"\n                      :error-messages=\"imgErrors\"\n                      label=\"Img\"\n                      prepend-icon=\"mdi-at\"\n                      required\n                      @input=\"$v.img.$touch()\"\n                      @blur=\"$v.img.$touch()\"\n                  ></v-text-field>\n                  <v-select\n                      outlined dense color=\"blue\"\n                      v-model=\"state\"\n                      :items=\"itemsState\"\n                      :error-messages=\"stateErrors\"\n                      label=\"Marital status\"\n                      prepend-icon=\"mdi-list-status\"\n                      required\n                      @change=\"$v.state.$touch()\"\n                      @blur=\"$v.state.$touch()\"\n                  ></v-select>\n                  <v-select\n                      outlined dense color=\"blue\"\n                      v-model=\"gender\"\n                      :items=\"itemsGender\"\n                      :error-messages=\"genderErrors\"\n                      label=\"Gender\"\n                      prepend-icon=\"mdi-human\"\n                      required\n                      @change=\"$v.gender.$touch()\"\n                      @blur=\"$v.gender.$touch()\"\n                  ></v-select>\n                  <v-text-field\n                      outlined dense color=\"blue\"\n                      v-model=\"phone\"\n                      :error-messages=\"phoneErrors\"\n                      :counter=\"9\"\n                      label=\"Phone Number\"\n                      prepend-icon=\"mdi-cellphone\"\n                      required\n                      @input=\"$v.phone.$touch()\"\n                      @blur=\"$v.phone.$touch()\"\n                  ></v-text-field>\n                  <v-menu\n                      ref=\"menu\"\n                      v-model=\"menu\"\n                      :close-on-content-click=\"false\"\n                      transition=\"scale-transition\"\n                      offset-y\n                      min-width=\"auto\">\n                    <template v-slot:activator=\"{ on, attrs }\">\n                      <v-text-field\n                          outlined dense color=\"blue\"\n                          v-model=\"date\"\n                          :error-messages=\"dateErrors\"\n                          required\n                          label=\"Birthday date\"\n                          prepend-icon=\"mdi-calendar\"\n                          readonly\n                          v-bind=\"attrs\"\n                          v-on=\"on\"\n                          @input=\"$v.date.$touch()\"\n                          @blur=\"$v.date.$touch()\"\n                      ></v-text-field>\n                    </template>\n                    <v-date-picker\n                        v-model=\"date\"\n                        :active-picker.sync=\"activePicker\"\n                        :max=\"(new Date(Date.now() - (new Date()).getTimezoneOffset() * 60000)).toISOString()\"\n                        min=\"1950-01-01\"\n                        @change=\"save\"\n                    ></v-date-picker>\n                  </v-menu>\n                </v-form>\n              </v-col>\n            </v-row>\n            <small class=\"ml-3\">*indicates required field</small>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"saveProfile()\">Save</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </template>\n  </div>\n</template>\n\n<script>\nimport PatientApiService from \"../../core/services/patient-api-service\"\nimport {validationMixin} from \"vuelidate\";\nimport {email, maxLength, required} from \"vuelidate/lib/validators\";\n\nexport default {\n  name: \"patient-profile\",\n  mixins: [validationMixin],\n  validations: {\n    firstName: { required, maxLength: maxLength(30) },\n    lastName: { required, maxLength: maxLength(30) },\n    phone: { required, maxLength: maxLength(9) },\n    email: { required, email },\n    state: { required },\n    gender: { required },\n    date: { required },\n    img: {required}\n  },\n  data: () => ({\n    patients: [],\n    profileData: [],\n    dataUser: {},\n    userId: 0,\n    dialog: false,\n    correct: null,\n    profileIndex: 0,\n    editedProfile: {\n      firstName:'',\n      lastName:'',\n      gender: null,\n      date: null,\n      state: null,\n      phone: ''\n    },\n    itemsState: [\n      'Single',\n      'Married',\n      'Widower',\n      'Divorced',\n    ],\n    itemsGender: [\n      'Male',\n      'Female',\n      'Others',\n    ],\n    activePicker: null,\n    menu: false\n  }),\n\n  watch: {\n    menu (val) {\n      val && setTimeout(() => (this.activePicker = 'YEAR'))\n    },\n  },\n\n\n  computed: {\n    firstNameErrors () {\n      const errors = []\n      if (!this.$v.firstName.$dirty) return errors\n      !this.$v.firstName.maxLength && errors.push('Name must be at most 30 characters long')\n      !this.$v.firstName.required && errors.push('Name is required.')\n      return errors\n    },\n  },\n\n  async created() {\n    this.userId = this.$route.params.id;\n    try {\n      const response = await PatientApiService.getAll();\n      const response2 = await PatientApiService.getById(this.userId);\n      this.patients = response.data;\n      this.profileData = response2.data;\n    } catch (e) {\n      console.error(e);\n    }\n\n    this.dialog = false;\n  },\n\n  methods: {\n\n    editProfile(select){\n      this.correct = select;\n      this.firstName = this.profileData.firstName;\n      this.lastName = this.profileData.lastName;\n      this.email = this.profileData.email;\n      this.gender = this.profileData.gender;\n      this.phone = this.profileData.phone;\n      this.state = this.profileData.state;\n      this.date = this.profileData.date.slice(0, 10).replace('T', ' ');\n      this.img = this.profileData.img;\n      this.dialog = true;\n    },\n\n    saveProfile() {\n     this.profileData.firstName = this.firstName;\n     this.profileData.lastName = this.lastName;\n     this.profileData.email = this.email;\n     this.profileData.gender = this.gender;\n     this.profileData.phone = this.phone.toString();\n     this.profileData.state = this.state;\n     this.profileData.date = this.date;\n     this.profileData.img = this.img;\n     console.log(this.profileData);\n     PatientApiService.update(this.profileData.id, this.profileData);\n     this.dialog = false;\n    },\n    save (date) {\n      this.$refs.menu.save(date)\n    }\n\n  }\n}\n</script>\n\n<style scoped>\n.dateProfile {\n  margin-left: 40px;\n}\n\n.user {\n  border-radius: 20%;\n}\n</style>"],"sourceRoot":"src/components/patients"}]}